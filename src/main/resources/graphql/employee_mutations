  }
}

mutation MODIFY_EMPLOYEE{
  up1: updateEmployee(id: "employee-2", firstName: "Pepe", lastName: "Pepe", dob: "11/30/2000", email: "pep@test.com", skill: "Sales", level: "Beginner", active: false){
    id
    firstName
    lastName
    dob
    email
    skill
    level
    active
  }
}

mutation DELETE_EMPLOYEE{
  del1: deleteEmployee(id: "employee-5"){
    id
    firstName
    lastName
    dob
    email
    skill
    level
    active
  }
}

